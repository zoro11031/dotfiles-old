#Permanently Floating Windows
for_window [title="Migaku Dictionary"] floating enable
for_window [app_id="pavucontrol"] floating enable, resize set 800 500
for_window [app_id="catfish"] floating enable, resize set 1000 450
for_window [class="Spotify"] mark "music", floating enable, resize set 1500 1000, move scratchpad
for_window [class="Bitwarden"] floating enable, mark "passwords"
for_window [title="Logs"] floating enable, resize set 500 1000
for_window [title="http:\/\/localhost:55193 is sharing a window."] move scratchpad

